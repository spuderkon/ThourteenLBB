<Application x:Class="FourteenLBB.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FourteenLBB" xmlns:Converters="clr-namespace:FourteenLBB.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Converters:DiscountConverter x:Key="DiscountConverter"/>
        <Converters:DiscountToStrikelineConverter x:Key="StrikelineConverter"/>
        <Converters:DiscountFormula x:Key="DiscountFormula"/>
    </Application.Resources>
</Application>
